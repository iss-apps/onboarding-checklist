<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>no.isstavanger.setuphelper</string>
    <key>ProgramArguments</key>
    <array>
        <string>{{HOME}}/Library/Application Support/Tools/URL Handler.app/Contents/Resources/Setup Helper</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/setup-helper.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/setup-helper.log</string>
</dict>
</plist>
